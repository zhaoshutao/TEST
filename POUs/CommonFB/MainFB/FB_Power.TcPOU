<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.6">
  <POU Name="FB_Power" Id="{d3a7443c-2919-46ae-a7ad-f8f713ca36af}" SpecialFunc="None">
    <Declaration><![CDATA[(****************************************************************
NAME:			FB_PowerOn
DATA:			2017.2.16
VERSION:		1.0.0
DESCRIPTION:	FB_PowerOn
HISTORY:
				2017.2.16		NEW

***************************************************************)
FUNCTION_BLOCK FB_Power
VAR_INPUT
	On:BOOL;
	Off:BOOL;
	Check:BOOL;
END_VAR
VAR_OUTPUT
	FbID:DINT:=1001;
	OnState:DINT;
	OffState: INT;
	OnBusy:BOOL;
	OffBusy:BOOL;
	OnStatus:BOOL;
	OffStatus: BOOL;
	Error:BOOL;
	ErrorID:DINT;
	PowerState:BOOL;
END_VAR
VAR
	LeftTopLin_MC_Power: MC_Power;
	RightTopLin_MC_Power: MC_Power;
	LeftBottomLin_MC_Power: MC_Power;
	RightBottomLin_MC_Power: MC_Power;
	fb_ton:TON;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[(*power on *)
IF On AND NOT Off THEN
	A_PowerOn();
(*Power Off *)
ELSIF Off AND NOT On THEN
	A_PowerOff();
END_IF
(*Check*)
IF Check THEN
	A_CheckPowerStatus();
END_IF]]></ST>
    </Implementation>
    <Action Name="A_CheckPowerStatus" Id="{45ab49c5-f74b-4ae0-bfbf-f6d250f649c1}">
      <Implementation>
        <ST><![CDATA[A_PowerMC();
IF  LeftTopLin_MC_Power.Status AND RightTopLin_MC_Power.Status
		AND LeftBottomLin_MC_Power.Status AND RightBottomLin_MC_Power.Status THEN
	PowerState:=1;
ELSIF NOT LeftTopLin_MC_Power.Status AND NOT RightTopLin_MC_Power.Status
		AND NOT LeftBottomLin_MC_Power.Status AND NOT RightBottomLin_MC_Power.Status THEN
	PowerState:=0;
ELSE
	PowerState:=0;
	Error:=TRUE;
	ErrorID:=77;
	(*FB_PlcEmergencyStop();*)
END_IF]]></ST>
      </Implementation>
    </Action>
