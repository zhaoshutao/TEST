    <Action Name="A_GapMove" Id="{ea782744-09ee-4428-bf59-993d9a34c853}">
      <Implementation>
        <ST><![CDATA[IF GapMove THEN
GapBusy:=TRUE;
GapDone:=FALSE;
fb_ton(PT:=t#80s);(*2mm/s max spees,120mmÐÐ³Ì£¬60sÄÚ×ßÍê*)
(*×îºóÔÙ¼ì²éÒ»±éñîºÏ*)
IF LeftTopLin.NcToPlc.CoupleState<>1 OR LeftBottomLin.NcToPlc.CoupleState<>1
			OR RightTopLin.NcToPlc.CoupleState<>3 OR RightBottomLin.NcToPlc.CoupleState<>3 THEN
			ErrorID:=1;
			GapState:=-100;
END_IF
A_MoveMC();
CASE GapState OF

	0:
		IF LeftTopLin.Status.Moving OR RightTopLin.Status.Moving
			 OR LeftBottomLin.Status.Moving OR RightBottomLin.Status.Moving THEN	(*ÖáÕýÔÚÔËÐÐ£¬Ñ­»·¸Ã²½µÈ´ý*)
			GapState:=88;
		ELSIF Top_MC_MoveAbsolute.Error OR Bottom_MC_MoveAbsolute.Error THEN		(*¼ì²éMC Error*)
			ErrorID:=2;
			GapState:=99;
		ELSIF ABS(Pos_LeftTopLin-GapPosition/2)<0.002 AND  ABS(Pos_LeftBottomLin-GapPosition)<0.002 THEN	(*Ä¿±êÎ»ÖÃÓëµ±Ç°Î»ÖÃÒ»Ñù£¬²»¶¯*)
			GapState:=88;
		ELSIF leftTopLin.Status.NotMoving AND  leftBottomLin.Status.NotMoving
			 AND RightTopLin.Status.NotMoving AND RightBottomLin.Status.NotMoving THEN	(*Öá¶¼´¦ÓÚÍ£Ö¹×´Ì¬Ê±£¬×ªÏÂÒ»²½*)
			GapState:=1;
		END_IF
	1:
		Top_MC_MoveAbsolute.Position:=GapPosition/2;
		Top_MC_MoveAbsolute.Velocity:=GapVelocity/2;
		Bottom_MC_MoveAbsolute.Position:=GapPosition/2;
		Bottom_MC_MoveAbsolute.Velocity:=GapVelocity/2;
		Top_MC_MoveAbsolute.Execute:=TRUE;
		Bottom_MC_MoveAbsolute.Execute:=TRUE;
		fb_ton.IN:=TRUE;
		GapState:=2;
	2:
		(*IF leftTopLin.Status.Moving AND   leftBottomLin.Status.NotMoving THEN (*ÉÏÏÂÁº²»Í¬Ê±¶¯*)
			ErrorID:=3;
			GapState:=-100;
		ELSIF RightTopLin.Status.NotMoving AND RightBottomLin.Status.Moving THEN (*ÉÏÏÂÁº²»Í¬Ê±¶¯*)
			ErrorID:=4;
			GapState:=-100;
		*)
		IF  (LeftTopLin.Status.Moving AND  RightTopLin.Status.NotMoving)
			OR  (LeftTopLin.Status.Moving AND LeftBottomLin.Status.NotMoving)
			OR (LeftTopLin.Status.Moving AND  RightBottomLin.Status.NotMoving)
				OR (RightTopLin.Status.Moving AND  LeftTopLin.Status.NotMoving)
				OR (RightTopLin.Status.Moving AND  LeftBottomLin.Status.NotMoving)
				OR (RightTopLin.Status.Moving AND  RightBottomLin.Status.NotMoving)
					OR (LeftBottomLin.Status.Moving AND  LeftTopLin.Status.NotMoving)
					OR (LeftBottomLin.Status.Moving AND  RightTopLin.Status.NotMoving)
					OR (LeftBottomLin.Status.Moving AND  RightBottomLin.Status.NotMoving)
						OR (RightBottomLin.Status.Moving AND  LeftTopLin.Status.NotMoving)
						OR (RightBottomLin.Status.Moving AND  RightTopLin.Status.NotMoving)
						OR (RightBottomLin.Status.Moving AND  LeftBottomLin.Status.NotMoving) THEN			(*Ò»Öá¶¯£¬ÆäËüÖá²»¶¯£¬½ô¼±×´Ì¬*)
			ErrorID:=5;
			GapState:=-100;
		ELSIF NOT Top_MC_MoveAbsolute.Done OR NOT  Bottom_MC_MoveAbsolute.Done THEN		(*ÔË¶¯ÖÐ£¬¼ì²éDoneÐÅºÅ£¬Ã»ÓÐÔòÑ­»·£¬³¬Ê±±¨´í*)
			(*IF NOT  fb_ton.Q THEN
				GapState:=2;
			ELSE
				ErrorID:=5;
				GapState:=-100;
			END_IF*)
			GapState:=2;
		ELSIF Top_MC_MoveAbsolute.Done  AND  Bottom_MC_MoveAbsolute.Done THEN		(*ÔË¶¯Íê³É£¬×ªÏÂÒ»²½*)
				GapState:=88;
		END_IF
		(*½ÓÊÕµ½stopÐÅºÅÊ±£¬reset×´Ì¬*)
		IF GapReset THEN
			GapState:=89;
		END_IF

	88:
		GapMove:=FALSE;
		GapState:=0;
		GapBusy:=FALSE;
		GapDone:=TRUE;
		Error:=FALSE;
		ErrorID:=0;
