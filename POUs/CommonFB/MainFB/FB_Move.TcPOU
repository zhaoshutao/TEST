			 AND LeftBottomLin.Status.Moving AND RightBottomLin.Status.Moving THEN
	GapMoving:=TRUE;
ELSE
	GapMoving:=FALSE;
END_IF
CASE BCGapState OF
	0:
		IF LeftTopLin.Status.Moving OR RightTopLin.Status.Moving
			 OR LeftBottomLin.Status.Moving OR RightBottomLin.Status.Moving THEN	(*ÖáÕýÔÚÔËÐÐ£¬Ñ­»·¸Ã²½µÈ´ý*)
			BCGapState:=88;
		ELSIF Top_MC_MoveAbsolute.Error OR Bottom_MC_MoveAbsolute.Error OR
				RightTopLin_MC_MoveAbsolute.Error OR RightBottomLin_MC_MoveAbsolute.Error THEN	(*¼ì²éMC Error*)
			ErrorID:=2;
			BCGapState:=99;
		(*ELSIF ABS(Pos_LeftTopLin-GapPosition/2)<0.002 AND  ABS(Pos_LeftBottomLin-GapPosition)<0.002 THEN	(*Ä¿±êÎ»ÖÃÓëµ±Ç°Î»ÖÃÒ»Ñù£¬²»¶¯*)
			GapState:=88;*)
		ELSIF leftTopLin.Status.NotMoving AND  leftBottomLin.Status.NotMoving
			 AND RightTopLin.Status.NotMoving AND RightBottomLin.Status.NotMoving THEN	(*Öá¶¼´¦ÓÚÍ£Ö¹×´Ì¬Ê±£¬×ªÏÂÒ»²½*)
			BCGapState:=1;
		END_IF
	1:
		Top_MC_MoveAbsolute.Position:=BCLeftTopPosition;
		Top_MC_MoveAbsolute.Velocity:=BCLeftTopVelocity;
		Bottom_MC_MoveAbsolute.Position:=BCLeftBottomPosition;
		Bottom_MC_MoveAbsolute.Velocity:=BCLeftBottomVelocity;
		RightTopLin_MC_MoveAbsolute.Position:=BCRightTopPosition;
		RightTopLin_MC_MoveAbsolute.Velocity:=BCRightTopVelocity;
		RightBottomLin_MC_MoveAbsolute.Position:=BCRightBottomPosition;
		RightBottomLin_MC_MoveAbsolute.Velocity:=BCRightBottomVelocity;
		Top_MC_MoveAbsolute.Execute:=TRUE;
		Bottom_MC_MoveAbsolute.Execute:=TRUE;
		RightTopLin_MC_MoveAbsolute.Execute:=TRUE;
		RightBottomLin_MC_MoveAbsolute.Execute:=TRUE;
		fb_ton_BC.IN:=TRUE;
		BCGapState:=2;
	2:
		IF NOT Top_MC_MoveAbsolute.Done OR NOT  Bottom_MC_MoveAbsolute.Done
				OR NOT RightTopLin_MC_MoveAbsolute.Done OR NOT RightBottomLin_MC_MoveAbsolute.Done THEN		(*ÔË¶¯ÖÐ£¬¼ì²éDoneÐÅºÅ£¬Ã»ÓÐÔòÑ­»·*)
			BCGapState:=2;
		ELSIF Top_MC_MoveAbsolute.Done  AND  Bottom_MC_MoveAbsolute.Done AND RightTopLin_MC_MoveAbsolute.Done AND RightBottomLin_MC_MoveAbsolute.Done THEN		(*ÔË¶¯Íê³É£¬×ªÏÂÒ»²½*)
			BCGapState:=88;
		END_IF
		(*½ÓÊÕµ½stopÐÅºÅÊ±£¬reset×´Ì¬*)
		IF BCGapReset THEN
			BCGapState:=89;
		END_IF

	88:
		BCGapMove:=FALSE;
		BCGapState:=0;
		BCGapBusy:=FALSE;
		BCGapDone:=TRUE;
		Error:=FALSE;
		ErrorID:=0;
		Top_MC_MoveAbsolute.Execute:=FALSE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightTopLin_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton_BC.IN:=FALSE;
	89:
		BCGapMove:=FALSE;
		BCGapState:=0;
		BCGapBusy:=FALSE;
		BCGapDone:=FALSE;
		Error:=FALSE;
		ErrorID:=0;
		Top_MC_MoveAbsolute.Execute:=FALSE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightTopLin_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton_BC.IN:=FALSE;
	99:
		BCGapMove:=FALSE;
		BCGapState:=99;
		BCGapBusy:=FALSE;
		BCGapDone:=FALSE;
		Error:=TRUE;
		Top_MC_MoveAbsolute.Execute:=FALSE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightTopLin_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton_BC.IN:=FALSE;
	-100:
		FB_PlcEmergencyStop();
		BCGapMove:=FALSE;
		BCGapState:=-100;
		BCGapBusy:=FALSE;
		BCGapDone:=FALSE;
		Error:=TRUE;
		Top_MC_MoveAbsolute.Execute:=FALSE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightTopLin_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton_BC.IN:=FALSE;
END_CASE
END_IF]]></ST>
      </Implementation>
