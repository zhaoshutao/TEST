    <Action Name="A_BottomMove" Id="{b44a7ecd-72fc-446a-9274-88916c5022c8}">
      <Implementation>
        <ST><![CDATA[IF BottomMove THEN
BottomBusy:=TRUE;
BottomDone:=FALSE;
fb_ton2(PT:=t#80s);(*2mm/s max spees,120mmÐÐ³Ì£¬60sÄÚ×ßÍê*)
A_MoveMC();
CASE BottomState OF

	0:
		IF LeftTopLin.Status.Moving OR RightTopLin.Status.Moving
			 OR LeftBottomLin.Status.Moving OR RightBottomLin.Status.Moving THEN	(*ÖáÕýÔÚÔËÐÐ£¬Ñ­»·¸Ã²½µÈ´ý*)
			TopState:=88;
		ELSIF  Bottom_MC_MoveAbsolute.Error OR RightBottomLin_MC_MoveAbsolute.Error THEN		(*¼ì²éMC Error*)
			ErrorID:=2;
			BottomState:=99;
		ELSIF leftTopLin.Status.NotMoving AND  leftBottomLin.Status.NotMoving
			 AND RightTopLin.Status.NotMoving AND RightBottomLin.Status.NotMoving THEN	(*Öá¶¼´¦ÓÚÍ£Ö¹×´Ì¬Ê±£¬×ªÏÂÒ»²½*)
			BottomState:=1;
		END_IF
	1:
		Bottom_MC_MoveAbsolute.Position:=BottomPosition;
		Bottom_MC_MoveAbsolute.Velocity:=BottomVelocity;
		Bottom_MC_MoveAbsolute.Execute:=TRUE;
		RightBottomLin_MC_MoveAbsolute.Position:=BottomPosition;
		RightBottomLin_MC_MoveAbsolute.Velocity:=BottomVelocity;
		RightBottomLin_MC_MoveAbsolute.Execute:=TRUE;
		fb_ton2.IN:=TRUE;
		BottomState:=2;
	2:
		IF leftTopLin.Status.Moving OR RightTopLin.Status.Moving THEN	(*ÉÏÁº¶¯,ESTOP*)
			ErrorID:=3;
			BottomState:=-100;
		ELSIF  (LeftBottomLin.Status.Moving AND  RightBottomLin.Status.NotMoving)
				OR (RightBottomLin.Status.Moving AND  LeftBottomLin.Status.NotMoving) THEN
			ErrorID:=4;
			BottomState:=-100;
		ELSIF NOT Bottom_MC_MoveAbsolute.Done THEN		(*ÔË¶¯ÖÐ£¬¼ì²éDoneÐÅºÅ£¬Ã»ÓÐÔòÑ­»·£¬³¬Ê±±¨´í*)
			BottomState:=2;
		ELSIF Bottom_MC_MoveAbsolute.Done THEN		(*ÔË¶¯Íê³É£¬×ªÏÂÒ»²½*)
				BottomState:=88;
		END_IF
		(*½ÓÊÕµ½stopÐÅºÅÊ±£¬reset×´Ì¬*)
		IF BottomReset THEN
			BottomState:=89;
		END_IF

	88:
		BottomMove:=FALSE;
		BottomState:=0;
		BottomBusy:=FALSE;
		BottomDone:=TRUE;
		Error:=FALSE;
		ErrorID:=0;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton2.IN:=FALSE;
	89:
		BottomMove:=FALSE;
		BottomState:=0;
		BottomBusy:=FALSE;
		BottomDone:=FALSE;
		Error:=FALSE;
		ErrorID:=0;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton2.IN:=FALSE;
	99:
		BottomMove:=FALSE;
		BottomState:=99;
		BottomBusy:=FALSE;
		BottomDone:=FALSE;
		Error:=TRUE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton2.IN:=FALSE;
	-100:
		FB_PlcEmergencyStop();
		BottomMove:=FALSE;
		BottomState:=-100;
		BottomBusy:=FALSE;
		BottomDone:=FALSE;
		Error:=TRUE;
		Bottom_MC_MoveAbsolute.Execute:=FALSE;
		RightBottomLin_MC_MoveAbsolute.Execute:=FALSE;
		fb_ton2.IN:=FALSE;
END_CASE
END_IF
]]></ST>
      </Implementation>
