	1:
		RightTopLin_MC_MoveAbsolute.Position:=TaperPosition;
		RightTopLin_MC_MoveAbsolute.Velocity:=TaperVelocity;
		RightBottomLin_MC_MoveAbsolute.Position:=TaperPosition;
