			OR RightTopLin.NcToPlc.CoupleState<>0 OR RightBottomLin.NcToPlc.CoupleState<>0 THEN
			ErrorID:=1;
			TaperState:=-100;
END_IF
(*×ó²àÉÏÏÂÖá¶¯£¬ESTOP*)
